}

.card-contain .card-last {
  display: flex;
  height: 450rpx;
  flex-direction: column;
  align-items: left;
  margin-top: 40rpx;
}

.card-contain::before {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 4rpx;
  background-color: #eee;
}

.buttom {
