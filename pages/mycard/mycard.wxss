page {
  font-size: 28rpx;
  color: #3f4142;
  height: 100%;
}

.card-contain {
  height: 70%;
  padding: 20rpx 40rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.card {
  border: 1rpx solid #eee;
  background-color: #80c2ee;
  border-radius: 20rpx;
  padding: 30rpx 40rpx;
}

.card-back {
  height: 450rpx;
  border: 1rpx solid #eee;
  background-color: #80c2ee;
  border-radius: 20rpx;
  padding: 35rpx 40rpx;
}

.card-f-up {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: top;
}



.card-text1 {
  margin-top: 30rpx;
