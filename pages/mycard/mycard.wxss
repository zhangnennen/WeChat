  font-size: 40rpx;

}


.idcard {
  position: relative;
  height: 550rpx;
  width: 100%;
}

.idcard image {
  z-index: -1;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);



